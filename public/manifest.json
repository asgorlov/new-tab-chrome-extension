{
  "manifest_version": 3,
  "name": "New Tab Page",
  "description": "Расширение заменяет новую вкладку браузера на страницу поисковой системы с разнообразным функционалом",
  "homepage_url": "https://github.com/asgorlov/new-tab-chrome-extension",
  "version": "3.6.0",
  "permissions": [
    "storage",
    "geolocation",
    "unlimitedStorage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  }
}
